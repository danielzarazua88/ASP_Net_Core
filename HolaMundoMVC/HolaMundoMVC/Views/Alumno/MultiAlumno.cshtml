@model IEnumerable<Alumno>

@{
    ViewData["Title"] = "Informacion Alumno";
}
<h1>@ViewData["Title"]</h1>
<div>
    <a class="btn btn-success" href="~/Alumno/Create/">Agregar</a>
</div>
<table class="table table-dark table-hover table-sm" >
    <thead>
        <tr>
            <th scope="col">Nombre</th>
            <th scope="col">Id</th>
            <th scope="col">Acciones</th>
        </tr>
    </thead>
    <tbody>
            @foreach(var alumno in Model)
            {
            <tr>
                <td>@alumno.Nombre</td>
                <td>@alumno.Id</td>
                <td>
                    <a class="btn btn-primary" href="~/Alumno/Edit/@alumno.Id">Editar</a>
                    <a class="btn btn-danger" href="~/Alumno/Delete/@alumno.Id">Eliminar</a>
                </td>
            </tr>
            }
    </tbody>
</table>
<p><i>@ViewBag.Fecha</i></p>